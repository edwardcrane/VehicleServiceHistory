<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.airanza.vehicleservicehistory" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/a_icon_white_trans_border_96x96"
        android:label="@string/app_name"
        android:logo="@drawable/airanza_logo_aqua_trans"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FuelEventActivity"
            android:label="@string/title_activity_fuel_event" >
        </activity>
        <activity
            android:name=".ManageVehiclesActivity"
            android:label="Vehicles" >
        </activity>
        <activity
            android:name=".VehicleActivity"
            android:label="@string/title_activity_vehicle" >
        </activity>
    </application>

</manifest>
